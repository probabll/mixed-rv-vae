{
    "seed": 10,
    "batch_size": 500,
    "height": 28,
    "width": 28, 
    "output_dir": "neurips-mixed-rv/supp/onehotcat-vae",
    "data_dir": "tmp",
    "device": "cuda:0",
    "y_dim": 0,    
    "z_dim": 10,
    "prior_z": "onehotcat 1.0 0.0",
    "hidden_dec_size": 500,
    "posterior_z": "onehotcat 1.0 -10 10",
    "shared_concentrations": true,
    "mean_field": true,
    "hidden_enc_size": 500,    
    "epochs": 50,    
    "num_samples": 100,    
    "gen_opt": "adam",
    "gen_lr": 5e-4,
    "gen_l2": 0.0,
    "gen_p_drop": 0.0,  
    "inf_opt": "adam",
    "inf_lr": 5e-4,  
    "inf_l2": 1e-6,  
    "inf_p_drop": 0.0, 
    "grad_clip": 5.0
}
