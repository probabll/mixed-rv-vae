{
    "seed": 10,
    "batch_size": 200,
    "data_dir": "tmp",
    "height": 28,
    "width": 28, 
    "output_dir": "neurips-mixed-rv/IvI/mixed-vae",
    "device": "cuda:0",
    "y_dim": 10,    
    "prior_f": "gibbs 0.0",
    "prior_y": "dirichlet 1.0",
    "z_dim": 0,
    "hidden_dec_size": 500,
    "posterior_f": "gibbs -10 10",
    "posterior_y": "dirichlet 1e-3 1e3",
    "shared_concentrations": true,
    "mean_field": true,
    "hidden_enc_size": 500,    
    "epochs": 500,    
    "num_samples": 100,    
    "gen_opt": "adam",
    "gen_lr": 5e-4,
    "gen_l2": 0.0,
    "gen_p_drop": 0.0,  
    "inf_opt": "adam",
    "inf_lr": 5e-4,  
    "inf_l2": 1e-6,  
    "inf_p_drop": 0.0, 
    "grad_clip": 5.0,
    "training_samples": 1,
    "exact_marginal": false,
    "use_self_critic": true,
    "use_reward_standardisation": false
}
